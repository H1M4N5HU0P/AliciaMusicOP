{
  "name": "𝐀𝐥𝐢𝐜𝐢𝐚 𝐌𝐮𝐬𝐢𝐜 𝐁𝐨𝐭🎶🎸",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "repository": "https://github.com/H1M4N5HU0P/AliciaMusicOP",
  "logo": "https://telegra.ph/file/9e02c16cc92700e765af0.jpg",
  "keywords": [
    "MafiaMusicBot",
    "telegram",
    "userbot",
    "voicechat",
    "music",
    "python",
    "pyrogram",
    "pytgcalls",
    "tgcalls",
    "voip"
  ],  
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string get from https://replit.com/@H1M4N5HU0P/AliciaPyroStringSession#main.py",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": true,
      "value": ""
    },
    "BOT_USERNAME": {
      "description": "Your MusicPlayer Bot username without @.",
      "required": true,
      "value": ""
    },
    "BOT_PIC": {
      "description": "Your MusicPlayer Bot Pic Goto @AliciaGroup_bot send your bot pic and reply your bot pic and type /tm and send message after you get url pic url put in vars.",
      "required": true,
      "value": ""
    },    
    "SUPPORT_CHANNEL": {
      "description": "Your MusicPlayer Bot Support Channel Userame without @.",
      "required": true,
      "value": ""
    },
    "ASSISTANT_USERNAME": {
      "description": "Your MusicPlayer Bot Assistant Userame without @.",
      "required": true,
      "value": ""
    },        
    "SUPPORT_GROUP": {
      "description": "Your MusicPlayer Bot Support Group Userame without @.",
      "required": true,
      "value": ""
    },            
    "OWNER_USERNAME": {
      "description": "Bot Owner Userame without @.",
      "required": true,
      "value": ""
    },               
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "ARQ_API_KEY": {
      "description": "Get from @ARQRobot.",
      "required": true,
      "value": ""
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "10"
    }
  },
  
  "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  },{
    "url": "heroku/python"
  }]
}
